<!-- MESSAGE BOX-->
<div class="message-box animated fadeIn" data-sound="alert" id="mb-signout">
  <div class="mb-container">
    <div class="mb-middle">
      <div class="mb-title"><span class="fa fa-sign-out"></span>
       Log<strong>Out</strong>
      </div>
      <div class="mb-content">
        <p>Are you sure you want to log out?</p>
      </div>
      <div class="mb-footer">
        <div class="pull-right">
          <%= link_to 'Yes', destroy_user_session_path, method: :delete,class: 'btn btn-primary btn-lg'%>
          <button class="btn btn-default btn-lg mb-control-close" onclick='$("#mb-signout").hide()'>No
          </button>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- END MESSAGE BOX-->
